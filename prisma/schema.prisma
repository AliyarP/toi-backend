// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Response {
  id        Int      @id @default(autoincrement())
  name      String
  answer    String
  count     Int
  createdAt DateTime @default(now())
}
